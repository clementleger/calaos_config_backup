        </calaos:condition>
        <calaos:action type="standard">
            <calaos:output id="intern_4" val="" val_var="intern_1"/>
        </calaos:action>
    </calaos:rule>
    <calaos:rule name="Changement Consigne Jour" type="Salon">
        <calaos:condition type="standard" trigger="true">
            <calaos:input id="input_1" oper="==" val="true"/>
        </calaos:condition>
        <calaos:condition type="standard" trigger="true">
            <calaos:input id="intern_0" oper="==" val="changed"/>
        </calaos:condition>
        <calaos:condition type="standard" trigger="true">
            <calaos:input id="intern_2" oper="==" val="false"/>
        </calaos:condition>
        <calaos:action type="standard">
            <calaos:output id="intern_4" val="" val_var="intern_0"/>
